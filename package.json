{
  "name": "unconfig",
  "type": "module",
  "version": "0.4.4",
  "packageManager": "pnpm@9.4.0",
  "description": "A universal solution for loading configurations.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/unconfig#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/unconfig.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/unconfig/issues"
  },
  "keywords": [
    "config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./presets": {
      "import": "./dist/presets.mjs",
      "require": "./dist/presets.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10",
    "defu": "^6.1.4",
    "importx": "^0.3.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.14.9",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "lodash-es": "^4.17.21",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  }
}
